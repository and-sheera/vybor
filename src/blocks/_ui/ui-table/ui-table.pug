          td Парковочные места (2уровень)
          td 55, 56, 57, 58, 59, 60, 67, 68, 70, 71, 72, 75, 76, 77, 78, 81, 83, 84, 87, 88, 89, 90
          td 650 000
        tr
          td Парковочные места (2уровень)
          td 61, 85
          td 690 000
