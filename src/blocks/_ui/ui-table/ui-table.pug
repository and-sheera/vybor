mixin ui-table(mods)
  -
    const baseName = 'ui-table'
    let allMods = ''
    if (typeof mods !== 'undefined' && mods) {
      const modsList = mods.split(',')
      for (const element of modsList) {
        allMods += element.trim()[0] === '-' ? ' ' + baseName + element.trim() : ' ' + element.trim() + '__' + baseName
      }
      allMods = allMods.trim()
    }

  .ui-table(class=allMods)&attributes(attributes)
    table
      thead
        tr
          th ОБЪЕКТ
          th НОМЕР ПОМЕЩЕНИЯ
          th СТОИМОСТЬ, РУБ.
      tbody
        tr
          td Парковочные места (1 уровень)
          td 6, 17, 19, 20, 25, 26, 32
          td 500 000
        tr
          td Парковочные места (1 уровень)
          td 11, 21, 22, 23, 24, 27, 28, 29, 30, 34, 35, 38, 39, 40, 41
          td 690 000
        tr
          td Парковочные места (2уровень)
          td 64, 65
          td 480 000
