footer.footer
  .inner
    .footer__top
      ul.footer__menu
        each block in footerData.menu
          li.footer__block
            .footer__heading(class=(block.innerMenu ? 'footer__heading--arrow' : ''))
              a.footer__link(href=block.link) !{block.name}
            if block.innerMenu
              ul.footer__list
                each item in block.innerMenu
                  li.footer__item
                    a.footer__link(href=item.link) !{item.name}
      .footer__address
        a.footer__address-link(href='#') Офисы продаж
        .footer__address-txt.footer__address-txt--blue Коминтерновский район
        .footer__address-txt ул. Вл. Невского, 19
      .footer__connect
        .footer__tel
          a.footer__tel-link(href='tel:88005338129') 8 800 533 8 129
          .footer__tel-descr бесплатно по России
        .footer__call
          .footer__call-title График работы call-центра:
          .footer__call-schedule
            p пн-чт: 09:00 - 18:00; пт: 09:00 - 17:00
            p сб: 10:00 - 17:00; вс: выходной
        .footer__socials
          each item in footerData.socials
            a.footer__social(href=item.link)
              img(src=item.icon, alt='')
    .footer__bottom
      .footer__made Продвижение сайта
